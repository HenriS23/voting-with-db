generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native", "rhel-openssl-1.0.x"]
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Person {
  id        String  @id @default(cuid())
  email     String
  firstname String
  lastname  String
  votable   Boolean
  votes     Int
}
